[user]
	name = Aaron Taner
	email = mapkts@gmail.com
[core]
	editor = nvim
	autocrlf = false
[https]
	proxy = http://127.0.0.1:1081
[http]
	proxy = http://127.0.0.1:1081
[credential]
	helper = store
[merge]
	tool = nvim -d
[alias]
    st = status
    c  = commit -v
    cm = commit -v
	lg = log --graph --decorate --pretty=oneline --abbrev-commit 
    ls = log -1 HEAD
	df = diff
	dc = diff --cached
	ck = checkout
    sl = stash list
[pull]
	rebase = true
